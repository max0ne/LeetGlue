{
  "manifest_version": 2,
  "name": "Leetglue",
  "description": "Glue Leetcode with Github",
  "version": "0.1",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content_script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "js/injected.js"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "storage",
    "webRequest",
    "*://developer.mozilla.org/",
    "*://*.google.com/",
    "*://*.leetcode.com/",
    "debugger",
    "notifications",
    "webRequestBlocking",
    "cookies"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.leetcode.com/*"
    ]
  }
}